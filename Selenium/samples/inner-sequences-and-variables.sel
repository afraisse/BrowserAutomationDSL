Sequence goToCampus { 
	Sequence goToLink(link) {
		GoTo link
	}
	site = GetLink "http://campus.mines-nantes.fr"
	Exec goToLink(site)
	connexion = GetLink "connexion"
	czam = GetButton "Cliquez ici ou sur le logo C'zam pour vous identifier"
	Click connexion
	Click czam
}

Sequence goToOasis {
	Sequence goToLink(link) {
		GoTo GetLink link
	}
	Exec goToLink("http://www.oasis.mines-nantes.fr")
}

Test "Firefox" {
	Exec goToCampus    
	Exec goToOasis
}
