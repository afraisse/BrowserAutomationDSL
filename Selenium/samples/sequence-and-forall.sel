Sequence goToGoogle {
	GoTo "http://www.google.com"
}

Sequence searchInGoogle(query) {
	Exec goToGoogle
	Fill "q" with query
	ForAll GetLinks link {
		GoTo link
	}
}

Test "Chrome" {
	Exec searchInGoogle("hello")
	Exec searchInGoogle("dsl")    
}

Test "Firefox" {
	Exec searchInGoogle("hi")
	Exec searchInGoogle("dsl")
}
